{
  "name": "qferr/mandrill-bundle",
  "description": "Symfony Mandrill Bundle",
  "type": "library",
  "authors": [
    {
      "name": "Quentin Ferrer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/http-foundation": "^6.0",
    "symfony/event-dispatcher": "^6.0",
    "symfony/http-kernel": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/config": "^6.0",
    "symfony/framework-bundle": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests/"
  },
  "autoload": {
    "psr-4": {
      "Qferrer\\Symfony\\MandrillBundle\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Qferrer\\Tests\\Symfony\\MandrillBundle\\": "tests/"
    }
  }
}